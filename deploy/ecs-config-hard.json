{
    "family": "prd-matrix",
    "containerDefinitions": [
      {
        "memory": MEMORY,
        "environment": [
            {
                "name": "ENVIRONMENT",
                "value": "VALUE"
            },
            {
              "name": "JAVA_XMX",
              "value": "SET_JAVA_XMX"
            }
        ],
        "name": "prd-matrix",
        "mountPoints": [],
        "image": "199886244715.dkr.ecr.us-east-1.amazonaws.com/matrix:latest",
        "cpu": CPU_LIMITS,
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": CONTAINER_PORT,
                "hostPort": HOST_PORT
            }
        ],
        "logConfiguration": {
            "logDriver": "syslog",
            "options": {
                "tag": "prd-matrix"
            }
        },
        "essential": true,
        "volumesFrom": []
      }
    ]
  }
